	     						<select class="percentChoice" name="${type}Change${year}" id="${type}Change${year}" onchange="Proforma.adjustForecast(this, '${type}','${year}');">
	     							<g:each in="${-50..50}" var="c">
	     								<g:if test="${c == value}">
			     							<option value="${c}" selected="selected">${c}%</option>
			     						</g:if>
			     						<g:else>
			     							<option value="${c}">${c}%</option>
			     						</g:else>
		     						</g:each>
	     						</select>